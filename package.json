  "bugs": {
    "url": "https://github.com/QGH11/QGH-Portfolio-Website/issues"
  },
  "homepage": "https://github.com/QGH11/QGH-Portfolio-Website#readme",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.2.1",
    "cannon": "^0.6.2",
    "gsap": "^3.11.4",
    "jquery": "^3.6.3",
    "owl.carousel": "^2.3.4",
    "three": "^0.120.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "babel-loader": "^9.1.2",
    "cannon-es": "^0.20.0",
    "imagemin-cli": "^7.0.0",
    "mini-css-extract-plugin": "^2.7.2",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "scripts": {
    "build:images": "imagemin client/assets/imgs/**/* --out-dir=dist/images",
    "build:js": "webpack --mode=production",
    "build": "run-p build:*",
    "watch:css": "onchange \"client/style\" -- npm run build:css",
    "watch:images": "onchange \"client/assets/imgs\" -- npm run build:images",
    "watch:js": "onchange \"client/js\" -- webpack --mode=development",
    "watch": "run-p serve watch:*"
  }
}
